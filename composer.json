{
    "name": "friendsofcake/bootstrap-ui",
    "description": "Bootstrap 3 support for CakePHP 4",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "bootstrap"],
    "homepage": "https://github.com/scherersoftware/bootstrap-ui",
    "license": "MIT",
    "authors": [
        {
            "name": "Jad Bitar",
            "homepage": "http://jadb.io",
            "role": "Author"
        },
        {
            "name": "Others",
            "homepage": "https://github.com/friendsofcake/bootstrap-ui/graphs/contributors"
        }
    ],
    "require": {
        "cakephp/cakephp": "^4.0.0"
    },
    "require-dev": {
        "cakephp/bake": "^2.0",
        "scherersoftware/coding-standard": "^4.0.0",
        "phpunit/phpunit": "~8.5.0"
    },
    "support": {
        "issues": "https://github.com/friendsofcake/bootstrap-ui/issues",
        "source": "https://github.com/friendsofcake/bootstrap-ui"
    },
    "autoload": {
        "psr-4": {
            "BootstrapUI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BootstrapUI\\Test\\": "tests/",
            "TestApp\\": "tests/test_app/TestApp/"
        }
    },
    "scripts": {
        "php-check": "vendor/bin/phpcs -p --standard=vendor/scherersoftware/coding-standard/scherersoftware ./src"
    }
}
